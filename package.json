{
  "name": "custom-field-maker",
  "version": "1.6.6",
  "description": "custom field maker for a-blog cms",
  "main": "lib/index.js",
  "files": [
    "dist",
    "lib",
    "css",
    "json",
    "screenshot.png"
  ],
  "type": "module",
  "target": "es5",
  "module": "commonjs",
  "overrides": {
    "tmp": "^0.2.5"
  },
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:babel": "babel src --out-dir lib --extensions .js,.jsx,.ts,.tsx",
    "build:json": "cp -r ./src/json ./lib/",
    "build:svgo": "svgo ./src/assets/images -o ./lib/assets/images",
    "build:prod": "NODE_ENV=production webpack --config webpack.prod.js",
    "dev": "webpack serve --config webpack.dev.js",
    "start": "npm-run-all -p dev",
    "prettier": "prettier ./src/**/*.js --write",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "deploy": "np --yolo --any-branch",
    "prepack": "npm run build",
    "prepare": "husky"
  },
  "author": "appleple",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.26.0",
    "classnames": "^2.5.1",
    "highlight.js": "~11.10.0",
    "html-entities": "^2.5.2",
    "js-beautify": "1.7.5",
    "react-copy-to-clipboard": "^5.1.0",
    "react-tooltip": "^5.28.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <19.0.0",
    "react-dom": ">=16.8.0 <19.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.25.9",
    "@babel/core": "^7.26.0",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/preset-react": "^7.25.9",
    "@babel/preset-typescript": "^7.26.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "babel-loader": "^9.2.1",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-webpack-plugin": "^4.2.0",
    "html-webpack-plugin": "^5.6.3",
    "husky": "^9.1.6",
    "jsdom": "^26.1.0",
    "np": "^10.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "style-loader": "^4.0.0",
    "svgo": "^3.3.2",
    "typescript": "^5.0.0",
    "vitest": "^3.2.4",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0",
    "webpack-merge": "^6.0.1"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.46.1"
  }
}
