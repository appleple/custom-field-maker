{
  "name": "custom-field-maker",
  "version": "1.5.24",
  "description": "custom field maker for a-blog cms",
  "main": "lib/index.js",
  "target": "es5",
  "module": "commonjs",
  "scripts": {
    "test": "jest ./tests/**.test.js",
    "build": "npm-run-all -p build:*",
    "build:babel": "babel src --out-dir lib",
    "build:prod": "NODE_ENV=production webpack --config webpack.prod.js",
    "dev": "webpack serve --config webpack.dev.js",
    "start": "npm-run-all -p dev",
    "prettier": "prettier ./src/**/*.js --write",
    "deploy": "np --no-cleanup --any-branch",
    "svgo": "svgo ./src/assets/images/*.svg ./lib/assets/images/*.svg",
    "prepare": "husky"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/"
  },
  "author": "steelydylan",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.8.4",
    "classnames": "^2.2.5",
    "highlight.js": "~9.13.1",
    "html-entities": "^1.2.1",
    "js-beautify": "~1.7.5",
    "react-copy-to-clipboard": "^5.0.1",
    "react-tooltip": "^4.1.5"
  },
  "peerDependencies": {
    "react": ">=16.8.0 <19.0.0",
    "react-dom": ">=16.8.0 <19.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.25.2",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.24.7",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^0.26.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-webpack-plugin": "^4.2.0",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "jest": "^25.1.0",
    "np": "^2.20.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^0.13.1",
    "svgo": "^1.0.5",
    "url-loader": "^0.5.7",
    "webpack": "^5.31.2",
    "webpack-cli": "^5.1.4",
    "webpack-merge": "^6.0.1"
  }
}
